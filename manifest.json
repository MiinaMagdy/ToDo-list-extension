{
    "name": "to do me",
    "manifest_version": 3,
    "version": "1.0",
    "description": "to do me is a simple todo list with add task button and delete checked checkbox tasks",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/icons/note16.png",
            "32": "/icons/note32.png",
            "64": "/icons/note64.png"
        }
    },
    "icons": {
        "16": "/icons/note16.png",
        "32": "/icons/note32.png",
        "64": "/icons/note64.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }]
}
